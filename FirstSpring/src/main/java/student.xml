<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- For injecting List via Constructor -->
	<bean id="student1" class="org.jsp.Student">
		<constructor-arg name="name" value="Devisri"></constructor-arg>
		<constructor-arg name="email" value="devil@gmail.com"></constructor-arg>
		<constructor-arg name="password" value="Devil@12345"></constructor-arg>
		<constructor-arg name="number" value="1231231231"></constructor-arg>
		<constructor-arg name="subjects" >
			<list>
				<value>Java</value>
				<value>Webtech</value>
				<value>Advance Java</value>
				<value>SQL</value>
			</list>
		
		
		</constructor-arg>
	</bean>
</beans>